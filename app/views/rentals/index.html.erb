    <h1>Rentals</h1>
    <%= link_to "Submit a rental!", new_rental_path %>
    <hr>
    <% @rentals.each do |rental| %>
    <div style="clear:both">
    	<div class="rental_img">
	    	<%= print_image rental.image_url.to_s %>
		</div>
	    <div class="rental_text">
	      <h2><%= link_to rental.desc, rental_path(rental) %></h2>
	      <h3><%= rental.location.name %></h3>
	    </div>
    </div>
      <hr style="clear:both">
    <% end %>