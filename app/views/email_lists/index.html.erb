<%= render 'admin/header' %>
<div class="container-fluid content_dashboard">

  <div class="table-responsive">
    <table class="table table-hover">
      <tr>
      
        <th>Name</th>

        <th>Email</th>
        <th>Sign up date</th>
        
      </tr>

       <% @emails.each do |list|  %>
      <tr>

        <td><%= list.name %></td>

        <td><%= list.email %></td>
        <td><%= list.created_at.strftime("%b %d, %Y")  %></td>
     
      </tr>
      <% end %>

    </table>
  </div>
  <div style="clear: both">
                <%= paginate @emails if @emails %>
                <%= page_entries_info @emails if @emails %>
            </div>
            <hr>
</div>