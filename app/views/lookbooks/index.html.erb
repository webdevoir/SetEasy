
   <div class="container">
   	  	<%= render 'header' %>
    <hr>

<div class="row rental-container">

    <% @lookbooks.each do |look| %>


                    <%= link_to location_lookbook_path(@set, look) do %>
                        <div class="rental_item">
                            <div class="rental_item_header">
                            <%= look.name %>
                            </div>
                            <div class="rental_item_pic">
                                <%= print_image look.image_url.to_s %>
                            </div>
                           

                        </div>
                     <% end %>
                <% end %>
            </div>
</div>